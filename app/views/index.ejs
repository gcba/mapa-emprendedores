		// http://localhost:3001/api/2014-11-03T20:45:24.0Z/2014-11-03T21:45:23.0Z
		var date = moment({ years:2010, months:3, days:5, hours:15, minutes:10, seconds:3, milliseconds:123})
		console.log(new Date(date._d).toISOString());

		var socket = io.connect('http://localhost');

		socket.on('connected', function(){
			console.log("conectado");
		});

		socket.on('update', function(data){
			console.log(data);
		});
			
		socket.on('time', function(t){
